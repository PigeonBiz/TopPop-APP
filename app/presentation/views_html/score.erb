<div class="container px-lg-5 main">
  <div>
    <% if videos.any? %>
      <div class="fs-4 fw-bold mgin">Your Score:<%= score %></div>
      <table>
        <tr>
          <th>Video</th>
          <th>Channel title</th>
          <th>Publish date</th>
          <th>View count</th>
          <th>Your Answer</th>
          <th>True Answer</th>
        </tr>
        <% videos.each do |video| %>
          <tr>
            <td>
              <a href="https://www.youtube.com/watch?v=<%= video.video_id %>">
                <%= video.title %>
              </a>  
            </td>
            <td><%= video.channel_title %></td>
            <td><%= video.publish_date %></td>
            <td><%= video.view_count %></td>
            <td><%= player_answer[video.video_id] %></td>
            <td><%= sort_answer[video.video_id] %></td>
          </tr>
        <% end %>
      </table>
    <% end %>
  </div>
</div>